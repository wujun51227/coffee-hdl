class AssignInFunction extends Module
  constructor: ->
    super()

    Port(
      din1: input(5)
      din2: input(5)
      din3: input(5)
      dout: output(5)
    )

  add:(v1,v2)->
    $ v1 + v2

  build: ->
    assign(@dout) => $ @din3+@add(@din1,@din2)

module.exports=AssignInFunction

