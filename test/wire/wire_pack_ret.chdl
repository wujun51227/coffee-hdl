class WirePackRet extends Module
  constructor: ->
    super()
    Port(
    )

    Wire(
      w1: [
        wire(5)
        wire(5)
        wire(5)
        wire(5)
        wire(5)
        ]
      w2: [
        wire(5)
        wire(5)
        wire(5)
        wire(5)
        wire(5)
        ]
    )

  build: ->
    assign([@w1[0],@w1[1]]) = [$(@w2[3]),$(@w2[4])]

    assign(@w1) = ($(i) for i in @w2)

module.exports=WirePackRet

