class NetSimple extends Module
  constructor: ->
    super()
    Port(
      din: input(5)
      dout: input(6)
    )

  build: ->
    Net(aaa,6) = @din
    Net bbb = @din
    Net(ccc)
    assign ccc = @din

module.exports=NetSimple

