someLib = require './somelib.json'
_ = require 'lodash'

class cell1 extends Module
  constructor: ->
    super()
    Port(
      bundle:
        sub:
          din: input(16)
          dout: output(16)
    )

  build: ->

class UseLib extends Module
  u0_cell1 : new cell1()
  u0_cell2 : new cell1()
  u0_cell3 : new cell1()
  constructor: ->
    super()

    @yyy=importLib('chdl_primitive_lib.chdl')

    a={
      b:
        c: input()
        d: [
          output(5)
          input(6)
          ]
    }
    Port(
      in: @yyy.createSigArray(input,10,10)
      a: a
      t:@mirrorPort(a)
      clk1: input(1).asClock()
    )

    template={
      e:
        f: [wire(),wire()]
    }

    Wire(
      w: @createSigArray(wire,10,32)
      select: wire(3)
      d: @bundleClone(template)
      g: @bundleClone(template)
      h: @bundleClone(template)
      syncin: wire(1)
      dout: wire(1)
    )

    Channel(
      x: @createChannelArray(10)
    )

    @u0_cell1.bind(
      bundle: @x[0]
    )

    @u0_cell2.bind(
      bundle: @x[1]
    )

    @u0_cell3.bind(
      bundle: @x[2]
    )

  build: ->
    $channelMux(@select,@x[0],@x[1],@x[2])
    $bundleUniOp('~',@g,@d)
    $bundleReduceOp('+',@d,[@g,@h])

    assign(@dout) = $oneHot($doubleSync(@syncin))

    assign(@dout) = $divider(@clk1,4)


module.exports=UseLib
