class ExprSimple extends Module
  constructor: ->
    super()
    Port(
      out: input(5)
      sel: input()
      out1: input()
      out2: input()
      out3: input()
    )

  build: ->
    data=100
    aaa = $opPrefix('^',@sel)
    assign(@out)
      $if(@sel) => $ {data+1}
      $else => hex(5,0x1f)
      $endif
    assign(@out1) = $ {$opPrefix('^',@sel)} + 1
    assign(@out2) = $ aaa + 1
    assign(@out3) = $opPrefix('^',@sel)

    console.log @out1.getDepNames()

module.exports=ExprSimple

