class ConstData extends Module
  constructor: ->
    super()
    Port(
      out1: output(16)
      out2: output(16)
      out3: output(16)
    )

  build: ->
    assign @out1 = 0+123+0x123+0o123+0b1100+hex(123)+hex(0x123)+oct(12,123)+dec(12,123)+bin(12,12)+12\h123+32\hffff55aa
    assign @out2
      $if(@out1(1)) => $ 100
      $else         => $ hex(200)

    assign @out3 = hex(16,32\h111)+0o37777777777+0x1234455aabbccdd+hex(64,BigInt('0x1222333444555666'))

module.exports=ConstData

